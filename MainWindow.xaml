<Window x:Class="_312551u5GameOfLife.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_312551u5GameOfLife"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <TextBox x:Name="txtInput" IsMouseDirectlyOverChanged="TxtInput_IsMouseDirectlyOverChanged">x,y</TextBox>
        <Button x:Name="btnAdd" Click="BtnAdd_Click">add</Button>
        <Button x:Name="btnStart" Click="BtnStart_Click">start</Button>

        <Canvas x:Name="canvas"/>
    </StackPanel>
</Window>
